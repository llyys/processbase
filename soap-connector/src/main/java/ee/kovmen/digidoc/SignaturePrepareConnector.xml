<connector>
  <connectorId>SignaturePrepareConnector</connectorId>
  <version>5.0</version>
  <icon>signature_start.ico</icon>
  <categories>
    <category>
      <name>DigiDoc</name>
      <icon>DigiDoc.ico</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setCert</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setFiles</setterName>
      <required></required>
      <parameters>
        <list/>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>signatureHash</name>
      <metadata/>
    </getter>   
  </outputs>
  <pages>
    <page>
      <pageId>Docs</pageId>
      <widgets>
        <text>
          <labelId>cert</labelId>
          <setter reference="/connector/inputs/setter"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <list>
          <labelId>files</labelId>
          <setter reference="/connector/inputs/setter[2]"/>
          <maxRows>0</maxRows>
        </list>
      </widgets>
    </page>
  </pages>
</connector>