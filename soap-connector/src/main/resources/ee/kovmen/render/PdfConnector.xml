<connector>
  <connectorId>PdfConnector</connectorId>
  <version>5.0</version>
  <icon>pdf.ico</icon>
  <categories>
    <category>
      <name>Documents</name>
      <icon>documents.ico</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setFile</setterName>
      <required></required>
      <parameters>
        <attachment/>
      </parameters>
    </setter>
  </inputs>
  <outputs/>
  <pages>
    <page>
      <pageId>PdfSettings</pageId>
      <widgets>
        <text>
          <labelId>file</labelId>
          <setter reference="/connector/inputs/setter"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
      </widgets>
    </page>
  </pages>
</connector>